#include <iostream>
#include <math.h>
#include <iomanip>
using namespace std;

int main() {
    int n; cin >> n;
    for(int i=0; i<n; i++) {
        int a; cin >> a;
        double d = 180*atan(5.3/a)/(22/7.0);
        int x = (int) (d*100 + 0.5);
        double n = (double) x/100; cout.setf(ios::fixed);
        cout << setprecision(2) << n << " degrees" << endl;
    }
}
